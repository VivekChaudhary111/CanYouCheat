{
  "version": 2,
  "name": "canyoucheat-frontend",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://canyoucheat.onrender.com",
    "REACT_APP_AI_SERVICE_URL": "https://canyoucheat-ai.onrender.com",
    "REACT_APP_SERVER_URL": "https://canyoucheat.onrender.com",
    "REACT_APP_FRAME_CAPTURE_RATE": "2"
  }
}