# React App Configuration
REACT_APP_API_URL=http://localhost:5000
REACT_APP_SERVER_URL=http://localhost:5000
REACT_APP_AI_SERVICE_URL=http://localhost:8000

# Development Settings
ESLINT_NO_DEV_ERRORS=true
DISABLE_ESLINT_PLUGIN=true
SKIP_PREFLIGHT_CHECK=true
GENERATE_SOURCEMAP=false

# Build Optimization
INLINE_RUNTIME_CHUNK=false

# Proctoring Configuration
REACT_APP_FRAME_CAPTURE_RATE=2
REACT_APP_RISK_THRESHOLD=70
REACT_APP_MAX_ALERTS=10

# Feature Flags
REACT_APP_DEBUG_MODE=true
REACT_APP_ENABLE_AI_PROCTORING=true
REACT_APP_ENABLE_FACE_DETECTION=true
REACT_APP_ENABLE_OBJECT_DETECTION=true

# Security
REACT_APP_ENABLE_STRICT_MODE=true

# Performance
REACT_APP_OPTIMIZE_IMAGES=true
REACT_APP_ENABLE_PWA=false